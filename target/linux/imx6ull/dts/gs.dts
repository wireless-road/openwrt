/dts-v1/;

#include <dt-bindings/input/input.h>
#include "imx6ull.dtsi"

/ {
	model = "Gs";
	compatible = "wirelessroad,stream-imx6ull", "fsl,imx6ull";

	aliases {
		cpu0 = &cpu0;
		can1 = &can1;
		clks = &clks;
		ecspi3 = &ecspi3;
		gpc = &gpc;
		gpmi = &gpmi;
		iomuxc = &iomuxc;
		pwm4 = &pwm4;
		adc1 = &adc1;
		usbotg1 = &usbotg1;
		usdhc1 = &usdhc1;
		usdhc2 = &usdhc2;
		wdog1 = &wdog1;
		uart3 = &uart3;
		uart5 = &uart5;
		uart6 = &uart6;
		i2c1 = &i2c1;
	};

	chosen {
		stdout-path = &uart1;
		bootargs = "console=ttymxc0,115200 rootwait fixrtc quiet";
	};

	gpio-keys {
		compatible="gpio-keys";
		label="Front panel buttons";

		start_1 {
			label = "Start 1";
			linux,code = <BTN_0>;
			gpios = <&gpio4 0 GPIO_ACTIVE_LOW>;
		};

		stop_1 {
			label = "Stopt 1";
			linux,code = <BTN_1>;
			gpios = <&gpio3 16 GPIO_ACTIVE_LOW>;
		};

		start_2 {
			label = "Start 2";
			linux,code = <BTN_2>;
			gpios = <&gpio3 6 GPIO_ACTIVE_LOW>;
		};

		stop_2 {
			label = "Stop 2";
			linux,code = <BTN_3>;
			gpios = <&gpio3 1 GPIO_ACTIVE_LOW>;
		};
	};

	memory@80000000 {
        device_type = "memory";
		reg = <0x80000000 0x20000000>;
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		reg_vref_3v3: regulator@0 {
			compatible = "regulator-fixed";
			regulator-name = "vref-3v3";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
		};
	};

	wfx_pwrseq: wfx_pwrseq {
		compatible = "mmc-pwrseq-simple";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_wfx_reset>;
		reset-gpios = <&gpio3 19 GPIO_ACTIVE_LOW>;
	};

	@aips3  {
		compatible = "fsl,aips-bus", "simple-bus";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x02200000 0x100000>;
		ranges;

		rngb: rngb@02284000 {
			compatible = "fsl,imx6sl-rng", "fsl,imx-rng", "imx-rng";
			reg = <0x02284000 0x4000>;
			interrupts = <GIC_SPI 6 IRQ_TYPE_LEVEL_HIGH>;
			clocks =  <&clks IMX6UL_CLK_DUMMY>;
		};
	};
};

#include "wr_cpu.dtsi"
#include "wr_partitions.dtsi"
#include "wr_usb.dtsi"
#include "wr_watchdog.dtsi"


&clks {
	assigned-clocks = <&clks IMX6UL_CLK_PLL4_AUDIO_DIV>;
	assigned-clock-rates = <786432000>;
};


&can1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan0>;
	status = "okay";
};


&gpc {
	fsl,cpu_pupscr_sw2iso = <0x1>;
	fsl,cpu_pupscr_sw = <0x0>;
	fsl,cpu_pdnscr_iso2sw = <0x1>;
	fsl,cpu_pdnscr_iso = <0x1>;
	fsl,ldo-bypass = <0>; /* DCDC, ldo-enable */
};

&iomuxc_snvs {
 	pinctrl-names = "default_snvs";
	default_snvs {};
 };

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;
	imx6ul-evk {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6UL_PAD_UART1_RTS_B__GPIO1_IO19	0x17059 /* SD1 CD */
				MX6UL_PAD_NAND_DQS__GPIO4_IO16		0x000010B0 /* TEMP IRQ */
				MX6UL_PAD_NAND_RE_B__GPIO4_IO00		0x00017040 /* Start 1 Pull up 47k */
				MX6UL_PAD_LCD_DATA11__GPIO3_IO16	0x00017040 /* Stop 1 Pull up 47k */
				MX6UL_PAD_LCD_DATA01__GPIO3_IO06	0x00017040 /* Start 2 Pull up 47k */
				MX6UL_PAD_LCD_ENABLE__GPIO3_IO01	0x00017040 /* Stop 2 Pull up 47k */
				MX6UL_PAD_NAND_WE_B__GPIO4_IO01		0x000010B0 /* BUZZ */
				MX6UL_PAD_NAND_WP_B__GPIO4_IO11		0x000010B0 /* K6 */
				MX6UL_PAD_NAND_CE1_B__GPIO4_IO14	0x000010B0 /* K5 */
				MX6UL_PAD_NAND_DATA07__GPIO4_IO09	0x000010B0 /* K4 */
				MX6UL_PAD_NAND_DATA06__GPIO4_IO08	0x000010B0 /* K3 */
				MX6UL_PAD_NAND_DATA05__GPIO4_IO07	0x000010B0 /* K2 */
				MX6UL_PAD_GPIO1_IO05__GPIO1_IO05	0x000010B0 /* K1 */
				MX6UL_PAD_GPIO1_IO09__GPIO1_IO09	0x000010B0 /* Warm */
				MX6UL_PAD_GPIO1_IO04__GPIO1_IO04	0x000010B0 /* Light */
				MX6UL_PAD_GPIO1_IO00__GPIO1_IO00	0x000010B0 /* Light 2 */
				MX6UL_PAD_GPIO1_IO01__GPIO1_IO01	0x000010B0 /* Warm 2 */
			>;
		};

		pinctrl_adc1: adc1grp {
			fsl,pins = <
				MX6UL_PAD_GPIO1_IO08__GPIO1_IO08        0xb0
			/*	MX6UL_PAD_GPIO1_IO09__GPIO1_IO09        0xb0*/
			>;
		};

		pinctrl_ecspi3_0: ecspi3grp-0 {
			fsl,pins = <
				MX6UL_PAD_UART2_CTS_B__ECSPI3_MOSI	0x110b1
				MX6UL_PAD_UART2_RTS_B__ECSPI3_MISO	0x110b1
				MX6UL_PAD_UART2_RX_DATA__ECSPI3_SCLK	0x110b1
				MX6UL_PAD_UART2_TX_DATA__GPIO1_IO20	0x10b0
			>;
		};

		pinctrl_i2c1: i2c1grp {
			fsl,pins = <
				MX6UL_PAD_UART4_TX_DATA__I2C1_SCL	0x4001b8b0
				MX6UL_PAD_UART4_RX_DATA__I2C1_SDA	0x4001b8b0
			>;
		};

		pinctrl_flexcan0: flexcan0grp{
			fsl,pins = <
				MX6UL_PAD_UART3_RTS_B__FLEXCAN1_RX	0x1b020
				MX6UL_PAD_UART3_CTS_B__FLEXCAN1_TX	0x1b020
			>;
		};

		pinctrl_pwm4: pwm4grp {
			fsl,pins = <
				MX6UL_PAD_GPIO1_IO05__PWM4_OUT   0x110b0
			>;
		};

		pinctrl_uart1: uart1grp {
			fsl,pins = <
				MX6UL_PAD_UART1_TX_DATA__UART1_DCE_TX 0x1b0b1
				MX6UL_PAD_UART1_RX_DATA__UART1_DCE_RX 0x1b0b1
			>;
		};

		pinctrl_uart3: uart3grp {
			fsl,pins = <
				MX6UL_PAD_NAND_READY_B__UART3_DCE_TX 0x1b0b1
				MX6UL_PAD_NAND_CE0_B__UART3_DCE_RX   0x1b0b1
			>;
		};

		pinctrl_uart5: uart5grp {
			fsl,pins = <
				MX6UL_PAD_UART5_TX_DATA__UART5_DCE_TX 0x1b0b1
				MX6UL_PAD_UART5_RX_DATA__UART5_DCE_RX 0x1b0b1
			>;
		};

		pinctrl_uart6: uart6grp {
			fsl,pins = <
				MX6UL_PAD_CSI_MCLK__UART6_DCE_TX   0x1b0b1
				MX6UL_PAD_CSI_PIXCLK__UART6_DCE_RX 0x1b0b1
			>;
		};

		pinctrl_usdhc1: usdhc1grp {
			fsl,pins = <
				MX6UL_PAD_SD1_CMD__USDHC1_CMD     0x17059
				MX6UL_PAD_SD1_CLK__USDHC1_CLK     0x10071
				MX6UL_PAD_SD1_DATA0__USDHC1_DATA0 0x17059
				MX6UL_PAD_SD1_DATA1__USDHC1_DATA1 0x17059
				MX6UL_PAD_SD1_DATA2__USDHC1_DATA2 0x17059
				MX6UL_PAD_SD1_DATA3__USDHC1_DATA3 0x17059
			>;
		};

		pinctrl_usdhc1_100mhz: usdhc1grp100mhz {
			fsl,pins = <
				MX6UL_PAD_SD1_CMD__USDHC1_CMD     0x170b9
				MX6UL_PAD_SD1_CLK__USDHC1_CLK     0x100b9
				MX6UL_PAD_SD1_DATA0__USDHC1_DATA0 0x170b9
				MX6UL_PAD_SD1_DATA1__USDHC1_DATA1 0x170b9
				MX6UL_PAD_SD1_DATA2__USDHC1_DATA2 0x170b9
				MX6UL_PAD_SD1_DATA3__USDHC1_DATA3 0x170b9
			>;
		};

		pinctrl_usdhc1_200mhz: usdhc1grp200mhz {
			fsl,pins = <
				MX6UL_PAD_SD1_CMD__USDHC1_CMD     0x170f9
				MX6UL_PAD_SD1_CLK__USDHC1_CLK     0x100f9
				MX6UL_PAD_SD1_DATA0__USDHC1_DATA0 0x170f9
				MX6UL_PAD_SD1_DATA1__USDHC1_DATA1 0x170f9
				MX6UL_PAD_SD1_DATA2__USDHC1_DATA2 0x170f9
				MX6UL_PAD_SD1_DATA3__USDHC1_DATA3 0x170f9
			>;
		};

		pinctrl_usdhc2: usdhc2grp {
			fsl,pins = <
				MX6UL_PAD_LCD_DATA18__USDHC2_CMD        0x1b0b0
				MX6UL_PAD_LCD_DATA19__USDHC2_CLK        0x100b0
				MX6UL_PAD_LCD_DATA20__USDHC2_DATA0      0x1b0b0
				MX6UL_PAD_LCD_DATA21__USDHC2_DATA1      0x1b0b0
				MX6UL_PAD_LCD_DATA22__USDHC2_DATA2      0x1b0b0
				MX6UL_PAD_LCD_DATA23__USDHC2_DATA3      0x1b0b0
			>;
		};
		
		pinctrl_wfx_irq: wfx_irq_grp {
			fsl,pins = <
				/* W_IRQ */
				MX6UL_PAD_LCD_DATA06__GPIO3_IO11        0x1b0b0
			>;
		};

		pinctrl_wfx_reset: wfx_reset_grp {
			fsl,pins = <
				/* W_RESET */
				MX6UL_PAD_LCD_DATA14__GPIO3_IO19        0x1b0b0
			>;
		};

		pinctrl_wfx_wakeup: wfx_wakeup_grp {
			fsl,pins = <
				/* W_UP */
				MX6UL_PAD_LCD_DATA08__GPIO3_IO13        0x130b0
			>;
		};

		pinctrl_wdog: wdoggrp {
			fsl,pins = <
				MX6UL_PAD_LCD_RESET__WDOG1_WDOG_ANY    0x30b0
			>;
		};

	};
};

&pwm4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm4>;
	status = "okay";
};

&adc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_adc1>;
	vref-supply = <&reg_vref_3v3>;
	num-channels = <1>;
	adc-ch-list = <8>;
	status = "okay";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3>;
	rts-gpios = <&gpio4 15 GPIO_ACTIVE_HIGH>;
	linux,rs485-enabled-at-boot-time;
	status = "okay";
};

&uart5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5>;
	rts-gpios = <&gpio4 25 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&uart6 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart6>;
	rts-gpios = <&gpio4 27 GPIO_ACTIVE_HIGH>;
	status = "okay";
};

&usbotg1 {
	maximum-speed = "full-speed";
	status = "okay";
};

&usdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc1>;
	cd-gpios = <&gpio1 19 GPIO_ACTIVE_LOW>;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	status = "okay";
};

&usdhc2 {  /* Wifi SDIO */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	no-1-8-v;
	non-removable;
	keep-power-in-suspend;
	mmc-pwrseq = <&wfx_pwrseq>;
	status = "okay";
	wfm200: wifi@1 {
		compatible = "silabs,wf200";
		reg = <1>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_wfx_irq &pinctrl_wfx_wakeup>;
		wakeup-gpios = <&gpio3 13 GPIO_ACTIVE_HIGH>;
		interrupts = <&gpio3 11 IRQ_TYPE_EDGE_RISING>;
	};
};

&i2c1 {
	clock_frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

	lm75: lm75@48 {
		compatible = "st,stlm75";
		reg = <0x48>;
		status = "okay";
	};

	ads1015: adc@49 {
		compatible = "ti,ads1115";
		reg = <0x49>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;

		channel@0 {
			reg = <0>;
		};

		channel@1 {
			reg = <1>;
		};
	};
};
